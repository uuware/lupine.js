{
  "name": "doc",
  "markdownEntryPoints": {
    "indir": "web/markdown",
    "outdir": "web/src/markdown-built"
  },
  "webEntryPoints": [
    {
      "index": "web/src/index.tsx",
      "html": "web/src/index.html",
      "outdir": "/"
    },
    {
      "index": "web/lupine.js/index.tsx",
      "html": "web/lupine.js/index.html",
      "outdir": "/lupine.js"
    }
  ],
  "apiEntryPoint": "api/src/index.ts",
  "watchFiles": ["web/markdown/**/*.md"],
  "copyFiles": [
    {
      "type": "web",
      "from": "web/assets",
      "to": "assets"
    },
    {
      "type": "web",
      "from": "web/lupine.js/assets",
      "to": "lupine.js/assets"
    },
    {
      "type": "web",
      "from": "web/lupine.js/404.html",
      "to": "lupine.js/404.html"
    },
    {
      "type": "data",
      "from": "api/resources",
      "to": "resources"
    }
  ]
}
