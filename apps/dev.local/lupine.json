{
  "name": "dev.local",
  "entryPoints": [
    {
      "index": "web/src/index.tsx",
      "html": "web/src/index.html",
      "outdir": "/"
    },
    {
      "index": "web/src/admin_dev/index.tsx",
      "html": "web/src/admin_dev/index.html",
      "outdir": "/admin_dev"
    }
  ],
  "entryPointApi": "server/src/index.ts",
  "watchFiles": ["web/assets/styles/global.css", "icons-font.config.js"],
  "copyFiles": [
    {
      "type": "web",
      "from": "web/assets",
      "to": "assets"
    },
    {
      "type": "data",
      "from": "server/resources",
      "to": "resources"
    }
  ]
}
